{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "description": "Testing env json",
    "required": [
        "BAR",
        "FOO"
    ],
    "properties": {
        "FOO": {
            "type": "string",
            "description": "description of FOO",
            "examples": ["foo1", "foo2"]
        },
        "BAR": {
            "type": "string",
            "description": "description of BAR",
            "enum": ["bar1", "bar2"]
        }
    }
}
